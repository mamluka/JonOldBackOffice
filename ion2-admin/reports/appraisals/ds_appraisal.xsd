<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="ds_appraisal" targetNamespace="http://tempuri.org/ds_appraisal.xsd" elementFormDefault="qualified" attributeFormDefault="qualified" xmlns="http://tempuri.org/ds_appraisal.xsd" xmlns:mstns="http://tempuri.org/ds_appraisal.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
    <xs:element name="ds_appraisal" msdata:IsDataSet="true">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element name="dbAppraisal">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="no" type="xs:string" minOccurs="0" />
                            <xs:element name="jewel_weight" type="xs:string" minOccurs="0" />
                            <xs:element name="jewel_description" type="xs:string" minOccurs="0" />
                            <xs:element name="jewel_middlestone" type="xs:string" minOccurs="0" />
                            <xs:element name="jewel_gold" type="xs:string" minOccurs="0" />
                            <xs:element name="stonetype" type="xs:string" minOccurs="0" />
                            <xs:element name="weight" type="xs:string" minOccurs="0" />
