                            <xs:element name="notes" type="xs:string" minOccurs="0" />
                            <xs:element name="picture" type="xs:base64Binary" minOccurs="0" />
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="dbUserInfo">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Name" type="xs:string" minOccurs="0" />
